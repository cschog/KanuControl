);\n    setBtnÄndernIsDisabled(false);\n    setPersonId(event.data.id);\n    setPerson(personen.filter((personen) => personen.id === event.data.id));\n  });\n\n  const editPerson = useCallback(() => {\n    setBtnLöschenIsDisabled(true);\n    setMitgliedFormEditMode(true);\n    setBtnÄndernIsDisabled(true);\n    setModusNeuePerson(false);\n  });\n\n  const deletePerson = useCallback(() => {\n    setBtnLöschenIsDisabled(true);\n    setBtnÄndernIsDisabled(true);\n    dbDeletePerson(personId);\n    setLoading(!loading);\n  });\n\n  const btnAbbruch = useCallback(() => {\n    setBtnLöschenIsDisabled(true);\n    setMitgliedFormEditMode(false);\n    initPerson(setPerson);\n  });\n\n  const btnNeuePerson = useCallback(() => {\n    // Felder leeren und auf Input umschalten\n    setModusNeuePerson(true);\n    setBtnLöschenIsDisabled(true);\n    setBtnÄndernIsDisabled(true);\n    setBtnNeuePersonIsDisabled(false);\n    setMitgliedFormEditMode(true);\n\n    initPerson(setPerson);\n  });\n\n  const btnSpeichern = useCallback((person) => {\n    setBtnLöschenIsDisabled(true);\n    setBtnÄndernIsDisabled(true);\n\n    if (modusNeuePerson) {\n      dbCreatePerson(person);\n    } else {\n      person.id = personId;\n      dbReplacePerson(person);\n    }\n    btnAbbruch();\n    setLoading(!loading);\n  });\n\n  const btnStartMenue = useCallback(() => {\n    // prüfen, ob die Daten plausibel sind (also nicht leer, Änderung gegenüber den vorherigen, ...)\n    window.location.href = \"/startmenue\";\n  });\n\n  useEffect(() => {\n    dbGetAllPersonen().then((personen) => {\n      setPersonen(personen);\n    });\n    dbGetAllVereine().then((vereine) => {\n      setVereine(vereine);\n    });\n    console.log(vereine);\n\n    initPerson(setPerson);\n  }, [loading]);\n\n  useEffect(() => {}, [person]);\n\n  const mitgliederAnz = personen.length; // Anzahl der Mitglieder\n\n  return (\n    <>\n      <MenueHeader headerText={mitgliederAnz + \" Mitglieder\"} />\n      <MitgliederTable\n        